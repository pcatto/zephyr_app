		"toolchainPrefix": "arm-zephyr-eabi",
		"request": "attach",
		"type": "cortex-debug",
		"runToEntryPoint": "main",
		"servertype": "openocd",
		"interface": "swd",
		"configFiles": [
			"/usr/share/openocd/scripts/interface/stlink-v2-1.cfg",
			"/usr/share/openocd/scripts/target/stm32g4x.cfg"
